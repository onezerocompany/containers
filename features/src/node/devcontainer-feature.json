{
  "name": "Node.js",
  "description": "Node.js with Yarn and PNPM package managers",
  "keywords": ["node", "javascript", "npm", "yarn", "pnpm"],
  "id": "node",
  "version": "1.0.2",
  "options": {
    "yarn": {
      "type": "boolean",
      "description": "Install Yarn package manager",
      "default": true
    },
    "pnpm": {
      "type": "boolean",
      "description": "Install PNPM package manager",
      "default": true
    },
    "version": {
      "type": "string",
      "description": "Node.js version",
      "default": "lts",
      "proposals": ["lts", "latest", "18", "20", "21"]
    },
    "global_packages": {
      "type": "string",
      "description": "Global packages to install (comma separated)",
      "default": ""
    }
  }
}
