{
  "name": "Google Cloud CLI",
  "id": "gcloud",
  "version": "1.1.1",
  "options": {
    "install": {
      "description": "Install the Google Cloud CLI",
      "type": "boolean",
      "default": true
    },
    "version": {
      "description": "Version of the Google Cloud CLI to install",
      "type": "string",
      "default": "latest"
    },
    "user": {
      "description": "User to install the Google Cloud CLI for",
      "type": "string",
      "default": "zero",
      "proposals": ["zero", "vscode", "root"]
    }
  },
  "customizations": {
    "vscode": {
      "extensions": ["GoogleCloudTools.cloudcode"]
    }
  }
}
