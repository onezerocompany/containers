{
  "name": "Bun Runtime",
  "description": "Bun runtime for JavaScript projects",
  "keywords": ["bun", "runtime", "javascript"],
  "id": "bun",
  "version": "1.0.6",
  "options": {
    "install": {
      "type": "boolean",
      "description": "Install bun",
      "default": true
    },
    "version": {
      "type": "string",
      "description": "Bun version",
      "default": "latest",
      "proposals": ["latest"]
    },
    "user": {
      "type": "string",
      "description": "User to run bun as",
      "default": "zero",
      "proposals": ["zero", "root", "vscode"]
    }
  }
}
