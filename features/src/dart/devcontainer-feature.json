{
  "name": "Dart",
  "description": "Dart SDKs",
  "keywords": ["dart"],
  "id": "dart",
  "version": "1.1.1",
  "options": {
    "install": {
      "type": "boolean",
      "description": "Install Dart SDK",
      "default": true
    },
    "version": {
      "type": "string",
      "description": "Dart version",
      "default": "stable",
      "proposals": ["stable", "beta", "dev"]
    },
    "user": {
      "type": "string",
      "description": "User to run the container as",
      "default": "zero",
      "proposals": ["zero", "vscode", "root"]
    }
  },
  "customizations": {
    "vscode": {
      "extensions": ["Dart-Code.dart-code"]
    }
  }
}
