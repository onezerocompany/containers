{
  "name": "GitHub CLI",
  "id": "github-cli",
  "version": "1.0.1",
  "options": {
    "install": {
      "description": "Install the GitHub CLI",
      "type": "boolean",
      "default": true
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "github.vscode-github-actions",
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "GitHub.vscode-pull-request-github"
      ]
    }
  }
}
